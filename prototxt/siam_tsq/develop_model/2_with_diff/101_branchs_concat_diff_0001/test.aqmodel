  top: "conv1_n"
  scale_param {
    bias_term: true
  }
  param{
    name: "conv1_scale_a1"
    lr_mult: 1
  }
  param{
    name: "conv1_scale_b1"
    lr_mult: 1
  }
}
layer {
  name: "relu1"
  type: "ReLU"
  bottom: "conv1_n"
  top: "conv1_n"
}
