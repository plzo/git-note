layer {
  name: "data"
  type: "Input"
  top: "data"
  input_param {
    shape {
      dim: 1
      dim: 6
      dim: 256
      dim: 256
    }
  }
}

layer{
  name: "slice_pair"
  type: "Slice"
  bottom: "data"
  top: "input_raw"
  top: "input_std"
  slice_param {
    axis: 1
    slice_point: 3
  }
}

layer{
  name: "input_diff"
  type: "Eltwise"
  bottom: "input_raw"
  bottom: "input_std"
  top: "input_diff"
  eltwise_param{
    operation: SUM
    coeff: 1
    coeff: -1
  }
}

layer{
  name: "input_diff_abs"
  type: "AbsVal"
  bottom: "input_diff"
  top: "input_diff_abs"

}

layer{
  name: "input_diff_abs_down8"
  type: "Interp"
  bottom: "input_diff_abs"
  top: "input_diff_abs_down8"
  interp_param{
    shrink_factor: 8
  }
}

##############################
layer {
  name: "conv1_n"
  type: "Convolution"
  bottom: "input_raw"
  top: "conv1_n"
  param{
    name: "conv1_n_w"
    lr_mult: 1
  }
  convolution_param {
    num_output: 32
    bias_term: false
    pad: 3
    kernel_size: 7
    stride: 2
    weight_filler {
      type: "msra"
    }   
    dilation: 1
  }
}

